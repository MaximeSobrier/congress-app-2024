{
  "name": "Website Classification",
  "description": "Classify web pages in real-time using a local machine-learning model.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
     "activeTab",
     "tabs",
     "scripting",
     "background",
     "nativeMessaging",
     "webNavigation",
     "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
     "service_worker": "background.js"
  },
  "action": {
    "default_title": "Classification Settings",
    "default_popup": "action.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "key": ""
}